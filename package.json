{
  "name": "multilife",
  "version": "1.1.0",
  "description": "Multiplayer game of life",
  "main": "server.js",
  "repository": "https://github.com/multilife",
  "author": "John Merchant <john@merchant.on.net>",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "blob-to-buffer": "^1.2.8",
    "color": "^3.1.2",
    "color-namer": "^1.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-github-corner": "^2.3.0",
    "react-redux": "^7.1.1",
    "react-share": "^3.0.1",
    "redux": "^4.0.4",
    "ws": "^7.1.2"
  },
  "scripts": {
    "test": "tsc && jest",
    "clean": "rimraf dist/",
    "build-client": "parcel build client/index.html --out-dir dist/client",
    "dev-client": "parcel serve client/index.html --out-dir dist/client",
    "build-server": "tsc --outDir dist/server && cpx process.yml dist/server/",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "dev-server": "npm run clean && npm run build-server && node dist/server/server"
  },
  "devDependencies": {
    "@types/blob-to-buffer": "^1.2.0",
    "@types/color": "^3.0.0",
    "@types/color-namer": "^1.3.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.11.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-share": "^3.0.0",
    "@types/ws": "^6.0.3",
    "cpx": "^1.5.0",
    "jest": "^24.9.0",
    "parcel": "^1.12.3",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  }
}
